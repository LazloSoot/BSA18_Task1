@model IEnumerable<Core.Entities.Post>

<h2>User posts</h2>

<section class="functionalityPanel">
    <table class="table" title="Target post">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Body</th>
                <th>Likes</th>
                <th>Created at</th>
                <th>User id</th>
                <th>Comments</th>
            </tr>
        </thead>
        <tbody>
                @foreach (var item in @Model)
                {
                    <tr>
                    @Html.Partial("_Post", item)
                    </tr>
                }
        </tbody>
    </table>
</section>
