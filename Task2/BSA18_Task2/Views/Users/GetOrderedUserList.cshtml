@model IEnumerable<Core.Entities.User>

<h2>All users</h2>

<section class="functionalityPanel">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Avatar</th>
                <th>Name</th>
                <th>Email</th>
                <th>Created at</th>
                <th>Posts</th>
                <th>Todos</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in @Model)
            {
                <tr>
                @Html.Partial("_User", item)
                </tr>
            }
        </tbody>
    </table>
</section>